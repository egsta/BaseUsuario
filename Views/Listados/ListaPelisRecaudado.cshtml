@{
    @model IEnumerable<BaseUsuario.Controllers.ListadosController.modelo_listaPelAlq>
}
@Html.Partial("EncabezadoListados")

 <h3>Listado de pelis por Pelis sumadas su recaudacion</h3>

 <table class="table">
    <thead>
        <tr>
            <th>
                 @Html.DisplayNameFor(model => model.Peli)
            </th>
            <th>
                  @Html.DisplayNameFor(model => model.Suma)
            </th>
            <th>
                  Operaciones
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Peli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Suma)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Count)
            </td>

        </tr>
}
    </tbody>
</table>