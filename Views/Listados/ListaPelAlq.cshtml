@model IEnumerable<BaseUsuario.Controllers.ListadosController.modelo_listaPelAlq>
@{

}

@Html.Partial("EncabezadoListados")

 <h3>Listado Pelis Alquiladas</h3>
 



<table class="table">
    <thead>
        <tr>
            <th>
               @Html.DisplayNameFor(model => model.Peli)
            </th>
            <th>
               @Html.DisplayNameFor(model => model.Usuario)
            </th>
             <th>
               @Html.DisplayNameFor(model => model.Precio)
            </th>
             <th>
               @Html.DisplayNameFor(model => model.Fecha)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Peli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>

        </tr>
}
    </tbody>
</table>